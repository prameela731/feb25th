# Online Python - IDE, Editor, Compiler, Interpreter
n=int(input("Enter n:"))
c=0
l=[]
t=[]
for i in range(n):
    x=int(input())
    l.append(x)
x=0
for i in range(n):
    if l[i]!=0:
        t.append(l[i])
        x+=1 
        c+=1
print("Count of non-zero numbres",c)
print(t)

